{
  "name": "exclusible-s-band-transponder",
  "version": "0.0.1",
  "description": "Websocket for the exchange rate for the pair BTC/USD",
  "main": "socket.ts",
  "scripts": {
    "ignition": "nodemon socket.ts",
    "prepare": "cd .. && husky install b-band-transponder/.husky",
    "prettier": "prettier --check --write .",
    "lint": "eslint . --ext .ts"
  },
  "engines": {
    "node": "~16",
    "npm": "~8"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "crypto",
    "ws"
  ],
  "author": "Nuno Alexandre",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.1",
    "express": "^4.17.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/ws": "^8.5.3",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "husky": "^7.0.4",
    "prettier": "2.6.2",
    "typescript": "^4.6.3"
  }
}
